import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        int wynik =0;

        for(int i=1; i<=a; i++)
        {
            wynik=wynik+i;
        }
        System.out.println(wynik);
    }
}
